{
  "name": "1x2-for-build-ts",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "generate": "npx prisma generate",
    "prisma-studio": "npx prisma studio",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "run-s migrate-dev db-seed && next dev -p 1004",
    "dx:prisma-studio": "npx prisma studio",
    "dx": "run-p dx:* --print-label",
    "dev": "next dev",
    "x": "next dev -p 1004",
    "prebuild": "run-s generate migrate",
    "build": "next build",
    "start": "next start",
    "lint": "lint . --ext .js,.ts,.tsx",
    "lint-fix": "lint . --fix",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test-unit": "vitest",
    "test-e2e": "playwright test",
    "test-start": "run-s test-unit test-e2e",
    "postinstall": "npm run generate",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6006 --silent",
    "serve-storybook": "run-s storybook:*",
    "test-storybook:ci": "start-server-and-test serve-storybook http://127.0.0.1:6006 test-storybook"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
